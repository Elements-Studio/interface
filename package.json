    "build": "yarn compile-contract-types && yarn i18n:extract && yarn i18n:compile && node scripts/build.js",
    "i18n:extract": "yarn lingui extract --locale en-US && yarn lingui extract --locale zh-CN",
    "i18n:compile": "lingui compile",
    "integration-test": "start-server-and-test 'serve build -l 3000' http://localhost:3000 'cypress run --record'",
    "graphql:generate": "graphql-codegen --config codegen.yml",
    "postinstall": "yarn compile-contract-types",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "prestart": "touch src/locales/en-US.po"
